{
  "name": "@jun-b/supabase-mcp-sf",
  "mcpName": "com.jun-b/supabase-mcp-sf",
  "version": "1.0.6",
  "description": "MCP server for self-hosted Supabase",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "test:unit": "vitest --project unit",
    "test:integration": "vitest --project integration",
    "test:coverage": "vitest --coverage"
  },
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "supabase-mcp-sf": "./dist/transports/stdio-selfhosted.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./platform": {
      "types": "./dist/platform/index.d.ts",
      "import": "./dist/platform/index.js",
      "default": "./dist/platform/index.cjs"
    },
    "./platform/selfhosted": {
      "types": "./dist/platform/selfhosted-platform.d.ts",
      "import": "./dist/platform/selfhosted-platform.js",
      "default": "./dist/platform/selfhosted-platform.cjs"
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0",
    "common-tags": "^1.8.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^22.8.6",
    "@vitest/coverage-v8": "^2.1.9",
    "dotenv": "^16.5.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.19",
    "vitest": "^2.1.9"
  }
}